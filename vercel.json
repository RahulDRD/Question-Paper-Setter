{
  "version": 2,
  "rewrites": [
    {
      "source": "/images/:path*",
      "destination": "/public/images/:path*"
    },
    {
      "source": "/:file(index\\.js|jquery\\.toast\\.js|jquery\\.toast\\.css)",
      "destination": "/public/:file"
    },
    {
      "source": "/:path*",
      "destination": "/api/server"
    }
  ]
}